<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DashBoard">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/lightbrownbg" />

    <ImageView
        android:layout_width="413dp"
        android:layout_height="155dp"
        android:background="@drawable/brownbg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="413dp"
        android:layout_height="155dp"
        android:background="@drawable/brownbg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:layout_width="400dp"
        android:layout_height="400dp"
        android:alpha="0.5"
        android:background="@drawable/logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/bg_chat"
        android:layout_width="175dp"
        android:layout_height="175dp"
        android:background="@drawable/brownroundbg"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/bg_reservation"
        android:layout_marginTop="225dp"/>
    <ImageView
        android:id="@+id/bg_reservation"
        android:layout_width="175dp"
        android:layout_height="175dp"
        android:background="@drawable/brownroundbg"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/bg_chat"
        android:layout_marginTop="225dp"/>
    <ImageView
        android:id="@+id/bg_order"
        android:layout_width="175dp"
        android:layout_height="175dp"
        android:background="@drawable/brownroundbg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/bg_settings"
        android:layout_marginBottom="225dp"/>

    <ImageView
        android:id="@+id/bg_settings"
        android:layout_width="175dp"
        android:layout_height="175dp"
        android:background="@drawable/brownroundbg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/bg_order"
        android:layout_marginBottom="225dp"/>

    <ImageButton
        android:id="@+id/btn_chat"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@drawable/chat_logo"
        app:layout_constraintBottom_toBottomOf="@id/bg_chat"
        app:layout_constraintEnd_toEndOf="@+id/bg_chat"
        app:layout_constraintStart_toStartOf="@+id/bg_chat"
        app:layout_constraintTop_toTopOf="@+id/bg_chat" />

    <ImageButton
        android:id="@+id/btn_reservation"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@drawable/reservation_logo"
        app:layout_constraintBottom_toBottomOf="@id/bg_reservation"
        app:layout_constraintEnd_toEndOf="@+id/bg_reservation"
        app:layout_constraintStart_toStartOf="@+id/bg_reservation"
        app:layout_constraintTop_toTopOf="@+id/bg_reservation" />

    <ImageButton
        android:id="@+id/btn_order"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@drawable/order_logo"
        app:layout_constraintBottom_toBottomOf="@id/bg_order"
        app:layout_constraintEnd_toEndOf="@+id/bg_order"
        app:layout_constraintStart_toStartOf="@+id/bg_order"
        app:layout_constraintTop_toTopOf="@+id/bg_order" />

    <ImageButton
        android:id="@+id/btn_setting"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@drawable/setting_logo"
        app:layout_constraintBottom_toBottomOf="@id/bg_settings"
        app:layout_constraintEnd_toEndOf="@+id/bg_settings"
        app:layout_constraintStart_toStartOf="@id/bg_settings"
        app:layout_constraintTop_toTopOf="@+id/bg_settings" />


</androidx.constraintlayout.widget.ConstraintLayout>